{
  "name": "ARCity",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "android-debug": "react-native run-android && exit 0",
    "android-debug-clean": "yarn clean-android && yarn android-debug",
    "android-alpha": "cd android && ./gradlew assembleReleaseAlpha && cd .. && yarn android-install-alpha",
    "android-alpha-clean": "yarn clean-android && yarn android-alpha",
    "android-release": "cd android && ./gradlew assembleRelease && cd .. && yarn android-install-release",
    "android-release-clean": "yarn clean-android && yarn android-release",
    "clean-android": "cd android && ./gradlew clean",
    "android-install-debug": "cd android/app/build/outputs/apk/debug && adb install -r app-debug.apk",
    "android-install-alpha": "cd android/app/build/outputs/apk/releaseAlpha && adb install -r app-releaseAlpha.apk",
    "android-install-release": "cd android/app/build/outputs/apk/release && adb install -r app-release.apk",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.34",
    "@react-navigation/native": "^5.0.0-alpha.24",
    "@react-navigation/stack": "^5.0.0-alpha.58",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-elements": "^1.2.7",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-google-places": "^3.1.2",
    "react-native-image-crop-picker": "^0.26.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-location": "^2.5.0",
    "react-native-maps": "^0.26.1",
    "react-native-modalbox": "^2.0.0",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-shadow": "^1.2.2",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-svg": "^11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prop-types": "^15.7.2",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
